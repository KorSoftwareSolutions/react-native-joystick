{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "react-native": ["./example/node_modules/react-native"],
      "react": ["./example/node_modules/react"],
      "react-native-gesture-handler": ["./example/node_modules/react-native-gesture-handler"]
    },
    "typeRoots": ["./example/node_modules/@types"],
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["ESNext"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmitOnError": true,
    "outDir": "lib",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "ESNext"
  }
}
